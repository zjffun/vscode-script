{
  "name": "script",
  "displayName": "Script",
  "description": "Create scripts with VS Code extension capabilities.",
  "keywords": [
    "script",
    "user script",
    "extension script"
  ],
  "version": "0.0.1",
  "publisher": "zjffun",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zjffun/vscode-script.git"
  },
  "bugs": {
    "url": "https://github.com/zjffun/vscode-script/issues"
  },
  "homepage": "https://github.com/zjffun/vscode-script#readme",
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "script.run",
        "title": "Run",
        "icon": "$(run)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "deploy": "vsce publish --yarn",
    "_vscode:pack": "vsce package",
    "_vscode:install": "ls script-*.vsix | xargs code --install-extension",
    "_vscode:uninstall": "code --uninstall-extension zjffun.script"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.2.0",
    "mocha": "^8.4.0",
    "typescript": "^4.5.5",
    "vsce": "^1.99.0",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "esm": "^3.2.25"
  },
  "icon": "images/logo.png"
}
